package json;

import java.lang.reflect.Array;

public class JSN {
	String p;
	String s;
	String h;
	String so;
	String str[][] = new String[][] { { "돌솥비빔밥", "1", "M", "H", "0" }, { "물냉면", "2", "L", "L", "1" },
			{ "비빔냉면", "1", "M", "L", "0" }, { "돼지국밥", "2", "M", "H", "1" }, { "육회비빔밥", "2", "L", "L", "0" },
