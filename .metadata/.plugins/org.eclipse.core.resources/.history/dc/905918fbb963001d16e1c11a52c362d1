package json;

public class JSN {
	String p;
	String s;
	String h;
	String so;
	String [][]arr = {
			{"이름", "가격", ""},
	};
	
	JSN(String p, String s, String h, String so) {
		this.p = p;
		this.s = s;
		this.h = h;
		this.so =so;
	}
	
	String pPrint() {
		return p;
	}
	String sPrint() {
		return s;
	}
	String hPrint() {
		return h;
	}
	String soPring() {
		return so;
	}
}


