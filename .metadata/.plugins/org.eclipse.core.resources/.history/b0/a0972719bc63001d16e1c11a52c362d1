package json;

public class JSN {
	String p;
	String s;
	String h;
	String so;
	
	public JSN(String p, String s, String h, String so) {
		this.p = p;
		this.s = s;
		this.h = h;
		this.so =so;
	}
	
	String pi() {
		return p;
	}
	String si() {
		return s;
	}
	String hi() {
		return h;
	}
	String soi() {
		return so;
	}
}


